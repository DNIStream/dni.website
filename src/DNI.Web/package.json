{
  "name": "dni",
  "version": "2.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build:dev": "ng build",
    "build:prod": "ng build --prod -c prod",
    "test": "ng test dni --watch",
    "lint": "ng lint dni",
    "lint:w": "chokidar *.ts src/**/*.ts src/**/*.htm* src/**/*.scss src/**/*.css src/**/*.json *.json -c \"ng lint dni\" --initial",
    "e2e": "ng e2e",
    "build:ssr-prod": "npm run build:client-and-server-bundles-prod && npm run webpack:server",
    "build:ssr-dev": "npm run build:client-and-server-bundles-dev && npm run webpack:server",
    "build:client-and-server-bundles-prod": "ng build --prod -c prod && ng run dni:server -c prod",
    "build:client-and-server-bundles-dev": "ng build && ng run dni:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr_bak": "node dist/server.js",
    "dev:ssr": "ng run dni:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run dni:server:production",
    "prerender": "ng run dni:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.1",
    "@angular/common": "^10.1.1",
    "@angular/compiler": "^10.1.1",
    "@angular/core": "^10.1.1",
    "@angular/forms": "^10.1.1",
    "@angular/platform-browser": "^10.1.1",
    "@angular/platform-browser-dynamic": "^10.1.1",
    "@angular/platform-server": "^10.1.1",
    "@angular/router": "^10.1.1",
    "@nguniversal/express-engine": "^10.1.0",
    "angular-pipes": "^9.0.2",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.2",
    "express": "^4.15.2",
    "jquery": "^3.5.1",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.28",
    "ngx-bootstrap": "^5.6.1",
    "ngx-captcha": "^8.0.1",
    "popper.js": "^1.14.7",
    "rxjs": "~6.5.3",
    "ts-loader": "^5.3.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.1",
    "@angular/cli": "^10.1.1",
    "@angular/compiler-cli": "^10.1.1",
    "@angular/language-service": "^10.1.1",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/moment-timezone": "^0.5.10",
    "@types/node": "^12.11.1",
    "chokidar-cli": "^2.1.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-cli": "^3.3.11"
  }
}
